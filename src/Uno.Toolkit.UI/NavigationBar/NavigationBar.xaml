	<Style x:Key="XamlDefaultNavigationBar"
		   TargetType="toolkit:NavigationBar">
		<Setter Property="ClosedDisplayMode"
				Value="Compact" />
		<Setter Property="LeftCommandStyle"
				Value="{StaticResource DefaultLeftCommandStyle}" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="toolkit:NavigationBar">
					<NavigationBarPresenter x:Name="NavigationBarPresenter" />
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="toolkit:NavigationBarPresenter">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="toolkit:NavigationBarPresenter">
					<muxc:CommandBar x:Name="XamlNavigationBarCommandBar"
									 Style="{StaticResource NavigationBarCommandBar}" />
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

