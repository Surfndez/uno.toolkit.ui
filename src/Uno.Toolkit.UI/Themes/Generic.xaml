<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
					xmlns:toolkit="using:Uno.UI.ToolkitLib"
					xmlns:ios="http://uno.ui/ios"
					xmlns:android="http://uno.ui/android"
					xmlns:netstdref="http://uno.ui/netstdref"
					mc:Ignorable="android ios netstdref">

	<!-- TabBar -->
	<Style TargetType="toolkit:TabBar"
		   BasedOn="{StaticResource DefaultTabBarStyle}" />

	<!-- TabBarItem -->
	<Style TargetType="toolkit:TabBarItem"
		   BasedOn="{StaticResource DefaultTabBarItemStyle}" />

	<!-- NavigationBar -->
	<ios:Style TargetType="toolkit:NavigationBar"
			   BasedOn="{StaticResource NativeDefaultNavigationBar}" />

	<android:Style TargetType="toolkit:NavigationBar"
				   BasedOn="{StaticResource NativeDefaultNavigationBar}" />

	<Style TargetType="toolkit:NavigationBar"
					 BasedOn="{StaticResource XamlDefaultNavigationBar}" />
	
</ResourceDictionary>
